{
  "extends": "stylelint-config-standard",
  "ignoreFiles": ["**/*.eot", "**/*.ttf", "**/*.woff"],
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": [true, {"ignoreAtRules": ["include","mixin","each"]}],
    "no-descending-specificity": [true, {"ignore": ["selectors-within-list"]}]
  }
}
